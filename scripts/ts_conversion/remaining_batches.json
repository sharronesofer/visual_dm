{
    "batches": [
        {
            "name": "batch3_core_utils",
            "description": "Core utility files from the src/core/utils directory",
            "source_dir": "src/core/utils",
            "files": [
                "**/*.ts"
            ],
            "excludes": [
                "**/*.spec.ts",
                "**/*.test.ts"
            ],
            "target_dir": "python_converted/src_batch3",
            "dependencies": [
                "types",
                "src/core/models"
            ]
        },
        {
            "name": "batch4_poi_utils",
            "description": "POI utility files from the src/poi/utils directory",
            "source_dir": "src/poi/utils",
            "files": [
                "**/*.ts"
            ],
            "excludes": [
                "**/*.spec.ts",
                "**/*.test.ts"
            ],
            "target_dir": "python_converted/src_batch4",
            "dependencies": [
                "types",
                "src/core/models",
                "src/poi/models"
            ]
        },
        {
            "name": "batch5_services",
            "description": "Service files from src/services directory",
            "source_dir": "src/services",
            "files": [
                "**/*.ts"
            ],
            "excludes": [
                "**/*.spec.ts",
                "**/*.test.ts"
            ],
            "target_dir": "python_converted/src_batch5",
            "dependencies": [
                "types",
                "src/core/models",
                "src/core/utils"
            ]
        },
        {
            "name": "batch6_api",
            "description": "API files from src/api directory",
            "source_dir": "src/api",
            "files": [
                "**/*.ts"
            ],
            "excludes": [
                "**/*.spec.ts",
                "**/*.test.ts"
            ],
            "target_dir": "python_converted/src_batch6",
            "dependencies": [
                "types",
                "src/core/models",
                "src/services"
            ]
        },
        {
            "name": "batch7_ui_models",
            "description": "UI model files from src/ui/models directory",
            "source_dir": "src/ui/models",
            "files": [
                "**/*.ts"
            ],
            "excludes": [
                "**/*.spec.ts",
                "**/*.test.ts"
            ],
            "target_dir": "python_converted/src_batch7",
            "dependencies": [
                "types",
                "src/core/models"
            ]
        },
        {
            "name": "batch8_remaining",
            "description": "All remaining TypeScript files",
            "source_dir": "src",
            "files": [
                "**/*.ts"
            ],
            "excludes": [
                "**/*.spec.ts",
                "**/*.test.ts",
                "core/models/**/*.ts",
                "poi/models/**/*.ts",
                "core/utils/**/*.ts",
                "poi/utils/**/*.ts",
                "services/**/*.ts",
                "api/**/*.ts",
                "ui/models/**/*.ts"
            ],
            "target_dir": "python_converted/src_batch8",
            "dependencies": [
                "types",
                "src/core/models",
                "src/poi/models",
                "src/services"
            ]
        }
    ],
    "global_settings": {
        "fix_imports": true,
        "create_init_files": true,
        "copy_comments": true,
        "add_type_annotations": true,
        "post_process": true
    }
}